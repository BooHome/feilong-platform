<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>com.feilong.platform.office</groupId>
		<artifactId>parent</artifactId>
		<version>1.11.1</version>
	</parent>

	<artifactId>feilong-office-excel</artifactId>
	<name>feilong-office-excel</name>
	<description>feilong-office-excel</description>

	<properties>
		<!-- **********************begin maven-compiler-plugin config******************************************* -->
		<maven-compiler-plugin.source>1.7</maven-compiler-plugin.source>
		<maven-compiler-plugin.target>1.7</maven-compiler-plugin.target>
		<version.loxia>2.5.2</version.loxia>

		<version.poi>3.16</version.poi>
		<!-- 新版poi需要这个jar -->
		<!-- <version.ooxml-schemas>1.1</version.ooxml-schemas> -->
	</properties>

	<dependencies>

		<dependency>
			<groupId>com.feilong.platform</groupId>
			<artifactId>feilong-io</artifactId>
			<version>${version.feilong-platform}</version>
		</dependency>

		<!-- -->
		<dependency>
			<groupId>com.feilong.platform</groupId>
			<artifactId>feilong-web-extension</artifactId>
			<version>${version.feilong-platform}</version>
			<scope>test</scope>
			<optional>true</optional>
		</dependency>


		<!-- ***************************** poi ***************************** -->
		<!-- poi HSSF is our port of the Microsoft Excel 97(-2007) file format (BIFF8) to pure Java. -->
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi</artifactId>
			<version>${version.poi}</version>
		</dependency>

		<!-- poi-ooxml XSSF is our port of the Microsoft Excel XML (2007+) file format (OOXML) to pure Java -->
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml</artifactId>
			<version>${version.poi}</version>
		</dependency>

		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml-schemas</artifactId>
			<version>${version.poi}</version>
		</dependency>

		<!--
			<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>ooxml-schemas</artifactId>
			<version>${version.ooxml-schemas}</version>
			</dependency>
		-->

		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-examples</artifactId>
			<version>${version.poi}</version>
			<optional>true</optional>
		</dependency>

		<dependency>
			<groupId>loxia2</groupId>
			<artifactId>loxia2-support</artifactId>
			<version>${version.loxia}</version>
			<exclusions>
				<exclusion>
					<groupId>org.springframework</groupId>
					<artifactId>spring-beans</artifactId>
				</exclusion>
				<exclusion>
					<artifactId>slf4j-log4j12</artifactId>
					<groupId>org.slf4j</groupId>
				</exclusion>
				<exclusion>
					<artifactId>slf4j-api</artifactId>
					<groupId>org.slf4j</groupId>
				</exclusion>
				<exclusion>
					<artifactId>spymemcached</artifactId>
					<groupId>spy</groupId>
				</exclusion>
				<!-- <exclusion>
					<artifactId>ognl</artifactId>
					<groupId>ognl</groupId>
					</exclusion> -->
				<exclusion>
					<groupId>com.sun.javax.media</groupId>
					<artifactId>jai_codec</artifactId>
				</exclusion>
				<exclusion>
					<groupId>com.sun.javax.media</groupId>
					<artifactId>jai_core</artifactId>
				</exclusion>
				<exclusion>
					<artifactId>jsoup</artifactId>
					<groupId>org.jsoup</groupId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>commons-digester</groupId>
			<artifactId>commons-digester</artifactId>
		</dependency>

		<dependency>
			<groupId>com.feilong.platform</groupId>
			<artifactId>feilong-core-extension</artifactId>
			<scope>test</scope>
		</dependency>
	</dependencies>
</project>